{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": ["es2018", "dom"],
    "paths": {
      "ui": ["dist/ui/ui", "dist/ui"],
      "@cartella/env/*": ["src/environments/*"],
      "@cartella/services/*": ["src/app/shared/services/*"],
      "@cartella/guards/*": ["src/app/shared/guards/*"],
      "@cartella/interfaces/*": ["src/app/shared/interfaces/*"],
      "@cartella/tokens/*": ["src/app/shared/tokens/*"],
      "@cartella/config/*": ["src/app/shared/config/*"],
      "@cartella/modules/*": ["src/app/shared/modules/*"],
      "@cartella/components/*": ["src/app/shared/components/*"],
      "@cartella/pipes/*": ["src/app/shared/pipes/*"],
      "@cartella/directives/*": ["src/app/shared/directives/*"],
      "@cartella/store/*": ["src/app/shared/store/*"],
      "@cartella/home/*": ["src/app/pages/dashboard/home/*"],
      "@cartella/bookmarks/*": ["src/app/pages/dashboard/bookmarks/*"],
      "@cartella/packages/*": ["src/app/pages/dashboard/packages/*"],
      "@cartella/snippets/*": ["src/app/pages/dashboard/snippets/*"]
    }
  }
}
