{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": ["es2018", "dom"],
    "paths": {
      "@cartella/ui": ["projects/ui/src/public-api.ts"],
      "@cartella/env/*": ["src/environments/*"],
      "@cartella/services/*": ["src/app/shared/services/*"],
      "@cartella/guards/*": ["src/app/shared/guards/*"],
      "@cartella/interfaces/*": ["src/app/shared/interfaces/*"],
      "@cartella/tokens/*": ["src/app/shared/tokens/*"],
      "@cartella/config/*": ["src/app/shared/config/*"],
      "@cartella/modules/*": ["src/app/shared/modules/*"],
      "@cartella/components/*": ["src/app/shared/components/*"],
      "@cartella/pipes/*": ["src/app/shared/pipes/*"],
      "@cartella/directives/*": ["src/app/shared/directives/*"],
      "@cartella/store/*": ["src/app/shared/store/*"],
      "@cartella/home/*": ["src/app/pages/dashboard/home/*"],
      "@cartella/snippets": ["projects/snippets/src/public-api.ts"],
      "@cartella/bookmarks": ["projects/bookmarks/src/public-api.ts"],
      "@cartella/packages": [
        "projects/packages/src/public-api.ts"
      ],
      "packages": [
        "dist/packages/packages",
        "dist/packages"
      ]
    }
  }
}
